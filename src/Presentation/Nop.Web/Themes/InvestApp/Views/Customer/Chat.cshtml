@using Nop.Core
@model CustomerInfoModel
@inject Nop.Core.IWebHelper webHelper
@{
    Layout = "_CustomLayout";

    //title
    NopHtml.AddTitleParts(T("PageTitle.ChatBot").Text);
    //page class
    NopHtml.AppendPageCssClassParts("html-login-page");
}

@*
<div class="page customer-stock-page">
</div> *@

<div class="page withdraw-page">
    <div class="page-body">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-12 page-title-header pb-1 mb-4">
                    <h2 class="page-title">@T("Account.ChatBot")</h2>
                </div>
            </div>
            <div class="row">        
                <div class="input-group col-xl-12">
                    <input type="text" class="form-control form-control-lg" id="small-searchterms" autocomplete="off" name="q" placeholder="@T("Chat.ChatBox.Tooltip")" aria-label="@T("Chat.ChatBox.Text.Label")" />
                    <button class="btn btn-outline-secondary" type="button"><i class="fa fa-message"></i></button>
                </div>               
            </div>
        </div>
    </div>
</div>

